{
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
        "no-console": "off", 
        "indent": ["error", 2], 
        "quotes": ["error", "single"], 
        "semi": ["error", "always"], 
        "comma-dangle": ["error", "never"], 
        "no-unused-vars": ["warn"], 
        "eqeqeq": ["error", "always"],
        "import/extensions": "off",
        "no-use-before-define": "off",
        "import/prefer-default-export": "off"
      }
  }